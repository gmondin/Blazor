@if (ToastList.Any())
  {
    <div class="toast-container position-@PositionClass">
      <CascadingValue Value=this>
        @foreach (var toast in ToastList.OrderBy(x => x.TimeStamp))
        {
          <ToastContent ToastSettings="toast.ToastSettings" ToastId="toast.Id" />
        }
      </CascadingValue>
    </div>
  }